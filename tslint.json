{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "object-literal-sort-keys": [false],
    "quotemark": [true, "single"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}